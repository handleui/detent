{
  "name": "@detent/api",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "build": "wrangler deploy --dry-run --outdir=dist",
    "deploy": "wrangler deploy --minify",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:setup": "./scripts/db-setup.sh",
    "db:setup:clean": "./scripts/db-setup.sh --clean",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@detent/healing": "*",
    "@detent/parser": "*",
    "@sentry/cloudflare": "^10.32.1",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.3",
    "jose": "^6.0.10",
    "pg": "^8.16.3",
    "resend": "^6.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250106.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "typescript": "5.9.2",
    "vitest": "^4.0.16",
    "wrangler": "^4.4.0"
  }
}
