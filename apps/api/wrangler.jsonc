{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "detent-api",
  "main": "src/index.ts",
  "minify": true,
  "compatibility_date": "2025-01-06",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    {
      "pattern": "api.detent.sh",
      "custom_domain": true
    }
  ],
  "hyperdrive": [
    {
      "binding": "HYPERDRIVE",
      "id": "d097d07d81dd449483d792aa860ec167",
      "localConnectionString": "${DATABASE_URL}"
    }
  ]
}
