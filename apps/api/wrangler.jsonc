{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "detent-api",
  "main": "src/index.ts",
  "minify": true,
  "compatibility_date": "2025-01-06",
  "compatibility_flags": ["nodejs_compat"],
  "routes": [
    {
      "pattern": "api.detent.sh",
      "custom_domain": true
    }
  ],
  "vars": {
    "WORKOS_SUBDOMAIN": "detent"
  }
}
